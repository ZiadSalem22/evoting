,"original":{"line":4,"column":20},"name":"Component"},{"generated":{"line":77,"column":18},"source":"components/Block.js","original":{"line":4,"column":29}},{"generated":{"line":78,"column":0},"source":"components/Block.js","original":{"line":73,"column":1}},{"generated":{"line":79,"column":0},"source":"components/Block.js","original":{"line":73,"column":2}},{"generated":{"line":79,"column":15},"source":"components/Block.js","original":{"line":75,"column":15},"name":"Block"},{"generated":{"line":79,"column":20},"source":"components/Block.js","original":{"line":75,"column":20}},{"generated":{"line":80,"column":0},"source":"components/Block.js","original":{"line":75,"column":20}}],"sources":{"components/Block.js":"import React, { Component } from \"react\";\r\nimport { Button } from \"react-bootstrap\";\r\n\r\nclass Block extends Component {\r\n\r\n    //to select if we want a breif display or deatiled  \r\n    state = {\r\n        displayTransaction: false\r\n    };\r\n\r\n    //to change the transaction display\r\n    toggleTransaction = () => {\r\n        this.setState({ displayTransaction: !this.state.displayTransaction });\r\n    }\r\n\r\n\r\n    //this is a computer function that returns any vaule we set for it in this case it returns a jsx div\r\n    get displayTransaction() {\r\n        const { data } = this.props.block;\r\n\r\n        const stringifiedData = JSON.stringify(data);\r\n\r\n        const dataDisplay = stringifiedData.length > 35 ?\r\n            `${stringifiedData.substring(0, 35)}...` :\r\n            stringifiedData;\r\n\r\n        if (this.state.displayTransaction) {\r\n            return (\r\n                <div>\r\n                    {JSON.stringify(data)}\r\n                    <br />\r\n                    <Button\r\n                        bsStyle=\"danger\"\r\n                        bsSize=\"small\"\r\n                        onClick={this.toggleTransaction}\r\n                    >Show Less\r\n                    </Button>\r\n                </div>\r\n            )\r\n        }\r\n\r\n\r\n        return (\r\n            <div>\r\n                <div>Data: {dataDisplay}</div>\r\n                <Button\r\n                    bsStyle=\"danger\"\r\n                    bsSize=\"small\"\r\n                    onClick={this.toggleTransaction}\r\n                >Show More\r\n                </Button>\r\n            </div>\r\n        );\r\n\r\n    }\r\n\r\n    render() {\r\n\r\n\r\n        const { timeStamp, hash } = this.props.block;\r\n\r\n        const hashDisplay = `${hash.substring(0, 15)}...`;\r\n\r\n\r\n        return (\r\n            <div className=\"Block\">\r\n                <div> Hash: {hashDisplay}</div>\r\n                <div> TimeStamp: {new Date(timeStamp).toLocaleString()}</div>\r\n                {this.displayTransaction}\r\n            </div>\r\n        );\r\n    }\r\n};\r\n\r\nexport default Block;"},"lineCount":null}},"error":null,"hash":"a39a34a8a44a612331b79bc547f0df34","cacheData":{"env":{}}}